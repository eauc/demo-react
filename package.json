      "react": "^16.2.0",
      "react-dom": "^16.2.0",
      "classnames": "^2.2.5",
      "react-router-dom": "^4.2.2",
      "react-bootstrap": "^0.32.1",
      "react-router-bootstrap": "^0.24.4",
      "redux": "^3.7.2",
      "redux-thunk": "^2.2.0",
      "reselect": "^3.0.1",
      "react-redux": "^5.0.7",
      "redux-form": "^7.3.0"
  },
  "devDependencies": {
